        <h3 class="modal-title login-modal-title" id="myModalLabel">Votree Login</h3>
      </div>
      <div id="login-modal" class="modal-body">
        <a href="/facebook"><img src="/resources/image/facebook.jpg" class="img-circle" alt="facebook" width="180" height="180"></a>
        <a href="/twitter"><img src="/resources/image/twitter.jpg" class="img-circle" alt="twitter" width="180" height="180"></a>
        <a href="/payco"><img src="/resources/image/payco.jpg" class="img-circle" alt="payco" width="180" height="180"></a>
      </div>
    </div>
  </div>
</div>
